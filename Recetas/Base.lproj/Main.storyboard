        <!--Receta Pastas al Alfredo-->
        <scene sceneID="6GF-HP-ZPk">
            <objects>
                <viewController id="Gik-hf-f69" sceneMemberID="viewController">
                    <layoutGuides>
                        <viewControllerLayoutGuide type="top" id="kDS-bC-7ec"/>
